<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.licenta">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_chat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_chat_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GoogleMapsActivity"></activity>

        <meta-data android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyC3k_XtiEeia0XS_WEOW6SzAOS644ot3kA"/>

        <activity android:name=".ConfirmAppointmentActivity" />
        <activity android:name=".AppointmentActivity" />
        <activity android:name=".SettingsMedicActivity" />
        <activity android:name=".SettingsPacientActivity" />
        <activity
            android:name=".WindowChatPatientActivity"
            android:parentActivityName=".StartMedicActivity" />
        <activity
            android:name=".WindowChatDoctorActivity"
            android:parentActivityName=".SettingsPacientActivity" />
        <activity android:name=".DoctorProfile" />
        <activity android:name=".StartMedicActivity" />
        <activity android:name=".StartPatientActivity" />
        <activity android:name=".PatientProfile" />
        <activity android:name=".Register.RegisterMedicActivity" />
        <activity android:name=".Register.RegisterPatientActivity" />
        <activity android:name=".Login.LoginPatientActivity" />
        <activity android:name=".Login.LoginMedicActivity" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>